{
  "name": "AdBlock Project",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Block trackers from most mainstream ad/analytic companies.",
  "browser_action": {
    "default_icon": "on.png"
  },
	"icons": { "16": "b.png",
           "48": "b.png",
					"128": "b.png"	},
  "background": {
    "scripts": ["filters.js", "block.js", "antifb.js"],
	"content_scripts": [ 
	  {
	  	"matches": [
	  		"<all_urls>"
	  	    ],
	  	"js": [""]
	  }  
]
  },
  "permissions": ["http://*/*", "https://*/*", "webRequest", "webRequestBlocking"],
  "incognito": "spanning",
  "web_accessible_resources": ["empty.gif", "blank.html"]
}
